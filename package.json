{
  "name": "typescript-monorepo",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm i && yarn build && yarn symlink-scripts && yarn start",
    "build": "pnpm recursive run build",
    "npm:publish": "pnpm publish",
    "start": "concurrently -p \"[{name}]\" -n \"React,JS,CRA\" -c \"black.bgMagenta,black.bgYellow,black.bgGreen\" \"pnpm start --filter=@tushar1998/react-library\" \"pnpm start --filter=@tushar1998/js-library\" \"pnpm start --filter=@tushar1998/cra\"",
    "symlink-scripts": "./symlink.sh"
  },
  "devDependencies": {
    "@types/node": "12.20.36",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
